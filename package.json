{
  "name": "nutrimprove",
  "version": "1.0.0",
  "description": "NutrImprove app",
  "main": "pages/index.js",
  "scripts": {
    "start": "next build && next start -p 4000",
    "dev": "next -p 4000",
    "deploy": "now",
    "deploy-test": "yarn deploy && yarn now alias nutrimprove",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --fix",
    "prettier": "prettier \"**/*.+(js|jsx|json|yml|yaml|css|less|scss|ts|tsx|md|mdx|graphql)\"",
    "format": "yarn prettier -- --write",
    "validate": "yarn lint && yarn prettier -- --list-different"
  },
  "engines": {
    "node": "^12"
  },
  "author": "Joao Jesus",
  "license": "UNLICENSED",
  "dependencies": {
    "@material-ui/core": "^4.9.11",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.50",
    "axios": "^0.19.0",
    "clsx": "^1.1.0",
    "downshift": "^3.2.14",
    "isomorphic-unfetch": "^3.0.0",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.19",
    "material-ui-popup-state": "^1.5.4",
    "mongodb": "^3.3.2",
    "mongoose": "^5.7.7",
    "next": "^9.4.2",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-loader-spinner": "^3.1.4",
    "react-promise-tracker": "^2.0.5",
    "react-redux": "^7.1.1",
    "react-use-auth": "^0.7.3",
    "react-virtualized": "^9.21.2",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "serve-favicon": "^2.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.0",
    "@babel/preset-env": "^7.10.0",
    "@testing-library/dom": "latest",
    "@testing-library/jest-dom": "^5.8.0",
    "@testing-library/react": "^10.0.4",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^26.0.1",
    "body-parser": "^1.19.0",
    "cross-env": "^5.2.1",
    "dotenv-webpack": "^1.7.0",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.2.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.17.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^3.0.5",
    "jest": "^26.0.1",
    "lint-staged": "^9.2.5",
    "nodemon": "^1.19.2",
    "now": "^17.1.1",
    "prettier": "^1.18.2",
    "react-test-renderer": "^16.13.1"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/.next/",
      "<rootDir>/node_modules/"
    ],
    "moduleNameMapper": {
      "^@[/](.+)": "<rootDir>/src/$1",
      "^components[/](.+)": "<rootDir>/components/$1",
      "^helpers[/](.+)": "<rootDir>/helpers/$1",
      "^store[/](.+)": "<rootDir>/store/$1"
    },
    "collectCoverageFrom": [
      "**/*.{js,jsx}"
    ]
  }
}
